{% layout 'layout.liquid' %}
{% block content %}
<div class="container-md">
    <article>
    
        <h1>
            {{ queen.name }} 
        </h1>
        <img src="{{queen.image_url}}" height="450" width="300" alt="">

        
        {% if example.owner == userId %}
        <div class="btn-group">
            <form action="/team/top5" method="POST">
                <input type="submit" value="She's Team Material" class="btn btn-dark">
            </form>
        </div>
        {% endif %}
        
    </article>
</div>
{% endblock %}